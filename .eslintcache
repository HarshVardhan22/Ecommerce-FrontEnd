\";\nimport Layout from \"../core/Layout\";\nimport {signIn,authenticate,isAuthenticated} from \"../Auth/index\";\nimport { Redirect } from \"react-router-dom\";\n\n\nconst SignIn =()=> {\n\n  const {user} = isAuthenticated()\n\n  const [values, setValues] = useState({\n    email: \"user3@gmail.com\",\n    password: \"user3user3\",\n    error: \"\", //if user has typo or something\n    loading: false,\n    redirectToReferrer: false\n  });\n\n  const {email,password,error,loading,redirectToReferrer} = values; //destructuring the object so that its easy to access the data inside\n\n  //handlechange is a higher order function i.e its a function which returns another function\n  //changed const handleChange = name => event => {\n  const handleChange = (property) => (event) => {\n    setValues({ ...values, error: false, [property]: event.target.value });\n  };\n\n\n  const clickSubmit = (event) => {\n\n    event.preventDefault();\n\n    setValues({...values,error:false,loading:true})                                                              \n\n    signIn({ email, password }).then((data) => {\n      if (data.error) {\n        setValues({ ...values, error: data.error, loading: false });\n      } else {\n        //this authenticate function is used to store data into the local server, for more info goto auth->index.js\n        //since the authenticate function had two arguments the first being data, we transfered the next() as a callback to set value\n        // of redirectToReferrer true which will lead the user to home page after the signIn is completed successfully\n        authenticate(data,()=>{\n          setValues({\n            ...values,\n            redirectToReferrer:true\n          });\n        })\n      }\n    });\n  }\n\n\n \n  const signInForm = () => \n    (\n      <form>\n        <div className=\" form-group pb-3 pt-3\">\n          <label className=\"text-muted\">Email</label>\n          <input\n            onChange={handleChange(\"email\")}\n            type=\"email\"\n            className=\"form-control\"\n            value = {email}\n          />\n        </div>\n\n        <div className=\" form-group pb-3\">\n          <label className=\"text-muted\">Password</label>\n          <input\n            onChange={handleChange(\"password\")}\n            type=\"password\"\n            className=\"form-control\"\n            value = {password}\n          />\n        </div>\n        <button onClick = {clickSubmit} className=\"btn btn-primary\">Submit</button>\n      </form>\n    );\n    \n    const showError = () =>\n      (<div className=\"alert alert-danger\" style={{display:error?'':'none'}}>\n        {error}\n      </div>);\n    \n    const showLoading = () =>\n      loading&&(<div className=\"alert alert-info\">\n        <h2>Loading...</h2>\n      </div>);\n    \n    const redirectUser=()=>{\n      if(redirectToReferrer){\n        if(user && user.role==1)\n          return <Redirect to=\"/admin/dashboard\"/>\n        else \n          return <Redirect to=\"/user/dashboard\"/>\n      }\n    }\n\n  return (\n    <div className=\"App\">\n      <Layout\n        title=\"Sign In!\"\n        description=\"Sign In page for E-commerce\"\n        className=\"container col-md-6 ofset-md-3\"\n      >\n        {showError()}\n        {showLoading()}\n        {signInForm()}\n        {redirectUser()}\n      </Layout>\n      {/* {JSON.stringify(values)} */}\n    </div>\n  );\n}\n\nexport default SignIn;\n","/home/harshvardhan/Desktop/code/ecommerce-frontend/src/user/SignUp.js",[],"/home/harshvardhan/Desktop/code/ecommerce-frontend/src/core/Home.js",[],"/home/harshvardhan/Desktop/code/ecommerce-frontend/src/core/Menu.js",["62","63"],"/home/harshvardhan/Desktop/code/ecommerce-frontend/src/core/Layout.js",[],"/home/harshvardhan/Desktop/code/ecommerce-frontend/src/Config.js",[],"/home/harshvardhan/Desktop/code/ecommerce-frontend/src/Auth/index.js",[],"/home/harshvardhan/Desktop/code/ecommerce-frontend/src/Auth/PrivateRoute.js",["64"],"import React, { Component } from 'react';\nimport {Route, Redirect} from 'react-router-dom';\nimport {isAuthenticated} from './index';\n\n//the link below explains the concept used behin this code in which we render a component based on a condition\n//https://reactrouter.com/web/example/auth-workflow\nconst PrivateRoute = ({component:Component,...rest}) =>(\n    <Route {...rest} render = { props => isAuthenticated() ? (\n        <Component {...props}/>\n    ) : (\n        <Redirect to={{pathname:'/signin', state:{from: props.location}}}/>\n    ) }/>\n)\n\nexport default PrivateRoute;","/home/harshvardhan/Desktop/code/ecommerce-frontend/src/user/UserDashboard.js",["65"],"import React from \"react\";\nimport Layout from \"../core/Layout\";\nimport {isAuthenticated} from \"../Auth/index\";\nimport {Link} from \"react-router-dom\"\nconst Dashboard = () =>{\n    \n    const {user} = isAuthenticated();\n\n    const userLinks = () => {\n        return (\n            <div className = 'card mt-5'>\n                <h4 className = \"card-header\">User Links</h4>\n                <ul className = 'list-group'>\n                    <li className=\"list-group-item\">\n                        <Link className= \"nav-link\" to=\"/cart\">My Cart</Link>\n                    </li>\n                    <li className=\"list-group-item\">\n                        <Link className= \"nav-link\" to=\"/profile/update\">Update Profile!</Link>\n                    </li>\n                </ul>\n            </div>\n        )\n    }\n\n    const userInfo = () => {\n      return (\n        <div className=\"card m-5\">\n          <h3 className=\"card-header\">User Information</h3>\n          <ul className=\"list-group\">\n            <li className=\"list-group-item\">{user.name}</li>\n            <li className=\"list-group-item\">{user.email}</li>\n            <li className=\"list-group-item\">\n              {user.role == 1 ? \"Admin\" : \"Registered User\"}\n            </li>\n          </ul>\n        </div>\n      );\n    };\n\n    const purchaseHistory = () => {\n      return (\n        <div className=\"card m-5\">\n          <h3 className=\"card-header\">Purchase history</h3>\n          <ul className=\"list-group\">\n            <li className=\"list-group-item\">ITEM 1</li>\n          </ul>\n        </div>\n      );\n    };\n\n    return(\n        <Layout title=\"Dashboard\" description={`Hi, ${user.name}!`} className=\"container-fluid\">\n          <div className=\"row\">\n              <div className=\"col-3\">\n                {userLinks()}\n              </div>\n              <div className=\"col-9\">\n                  {userInfo()}\n                  {purchaseHistory()}\n                  </div>\n          </div>  \n        </Layout> \n    )\n}\n\nexport default Dashboard;","/home/harshvardhan/Desktop/code/ecommerce-frontend/src/Auth/AdminRoute.js",["66","67"],"import React, { Component } from 'react';\nimport {Route, Redirect} from 'react-router-dom';\nimport {isAuthenticated} from './index';\n\n//the link below explains the concept used behin this code in which we render a component based on a condition\n//https://reactrouter.com/web/example/auth-workflow\nconst AdminRoute = ({component:Component,...rest}) =>(\n    <Route {...rest} render = { props => isAuthenticated() && isAuthenticated().user.role==1 ? (\n        <Component {...props}/>\n    ) : (\n        <Redirect to={{pathname:'/signin', state:{from: props.location}}}/>\n    ) }/>\n)\n\nexport default AdminRoute;","/home/harshvardhan/Desktop/code/ecommerce-frontend/src/user/AdminDashboard.js",["68"],{"ruleId":"69","replacedBy":"70"},{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":91,"column":29,"nodeType":"75","messageId":"76","endLine":91,"endColumn":31},{"ruleId":"73","severity":1,"message":"74","line":46,"column":58,"nodeType":"75","messageId":"76","endLine":46,"endColumn":60},{"ruleId":"73","severity":1,"message":"74","line":52,"column":58,"nodeType":"7